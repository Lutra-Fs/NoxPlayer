{
  "name": "电闹播放器",
  "version": "4.1.1.0",
  "description": "电闹播放器 / nox-player: 第三方b站播放器，阿梓播放器威力加强版 / 3rd party Bilibili audio player modified from Azusa-player.",
  "manifest_version": 3,
  "background": {
    "scripts": ["serviceWorker.js"]
  },
  "action": {
    "default_icon": {
      "16": "icons/logo2-128.png",
      "128": "icons/logo2-128.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": ["js/ffmpeg-core.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_security_policy": "script-src 'self'; object-src 'self';",
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["js/contentscript.js", "js/ffmpeg-core.js"]
    }
  ],
  "icons": {
    "128": "icons/logo2-128.png"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "cookies",
    "identity",
    "search"
  ],
  "host_permissions": [
    "*://*.acgvideo.com/*",
    "*://*.bilivideo.com/*",
    "*://*.bilibili.com/*",
    "*://*.hdslb.com/*",
    "*://*.cgvideo.com/*",
    "*://*.y.qq.com/*",
    "*://*.akamaized.net/*",
    "*://*.bilivideo.cn/*",
    "*://*.youtube.com/*"
  ]
}
